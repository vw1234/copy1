{% extends 'NwpAssessmentBundle::layout.html.twig' %}

{% block title %}
{{ parent() }} - ScoringItem {{ 'views.show.show'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
{% endblock %}

{% block page %}

<h3 class="title">ScoringItem</h3>

<form class="form-horizontal">
    <fieldset>        <div class="control-group">
            <label class="control-label">Id</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.id }}</span>            </div>
        </div>        
            <div class="control-group">
            <label class="control-label">Student Id</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.studentId }}</span>            </div>
        </div>        
             <div class="control-group">
            <label class="control-label">Administration Time</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.administrationTime }}</span>            </div>
        </div> 
             <div class="control-group">
            <label class="control-label">School Year</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.year }}</span>            </div>
        </div>  
             <div class="control-group">
            <label class="control-label">Project</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.project }}</span>            </div>
        </div>   
             <div class="control-group">
            <label class="control-label">Prompt</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.prompt }}</span>            </div>
        </div>   
             <div class="control-group">
            <label class="control-label">Grade Level</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.gradeLevel }}</span>            </div>
        </div>   
             <div class="control-group">
            <label class="control-label">Organization Type</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.organizationType }}</span>            </div>
        </div> 
            {% if entity.ncesId %}
             <div class="control-group">
            <label class="control-label">Nces Id</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.ncesId }}</span>            </div>
        </div>  
            {% endif %} 
            {% if entity.psId %}
             <div class="control-group">
            <label class="control-label">Ps Id</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.psId }}</span>            </div>
        </div>   
            {% endif %}
             {% if entity.districtId %}
             <div class="control-group">
            <label class="control-label">District Id</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.districtId }}</span>            </div>
        </div>    
            {% endif %}
            {% if entity.ipedsId %}
             <div class="control-group">
            <label class="control-label">Ipeds Id</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.ipedsId }}</span>            </div>
        </div>  
            {% endif %}
             <div class="control-group">
            <label class="control-label">Organization Name</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.organizationName }}</span>            </div>
        </div>    
             <div class="control-group">
            <label class="control-label">State</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.state }}</span>            </div>
        </div>    
             <div class="control-group">
            <label class="control-label">County</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.county }}</span>            </div>
        </div>    
            <div class="control-group">
            <label class="control-label">Classroom Id</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.classroomId }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Teacher Id</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.teacherId }}</span>            </div>
        </div>      
          <div class="control-group">
            <label class="control-label">Original Filename</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.originalFileName }}</span>            </div>
        </div>    
        <div class="control-group">
            <label class="control-label">Date File Uploaded</label>
            {% if (entity.dateUploaded) %}
            <div class="controls"> <a href="{{ path('projectsite_scoringitem_download', { 'id': entity.id }) }}" target="blank">{{entity.dateUploaded|date('Y-m-d H:i:s')}} </a></div>
        {% else %}
            <div class="controls">N/A</div>
        {% endif %}
        </div>    
        <div class="control-group"> 
             <label class="control-label">Attached File</label> 
        {% if ((entity.originalFileName) and (entity.dateUploaded)) %}
            <div class="controls"> <a href="{{ path('projectsite_scoringitem_download', { 'id': entity.id }) }}" target="blank">{{entity.originalFileName}}.{{entity.filetype}} </a></div>
        {% else %}
            <div class="controls">N/A</div>
        {% endif %}
        </div> 
        </fieldset>
</form>

<div>
    <div class="float-left">
        <a class="btn" href="{{ path('projectsite_scoringitem') }}">
            {{ 'views.recordactions.backtothelist'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
        </a>
    </div>
   
</div>
{% endblock %}